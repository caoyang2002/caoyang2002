 />
 &lt;button onclick="decrypt()" class="crypto-btn">解密&lt;/button>
 &lt;/div>
 
 &lt;div
 class="encrypted-content"
 id="crypto-output"
 data-content="Cjx1bD4KPGxpPgo8YSBocmVmPSJodHRwczovL3NpbmNlMTk4Mi1teS5zaGFyZXBvaW50LmNvbS9wZXJzb25hbC9zaW5jZTI1MDFfc2luY2UxOTgyX29yZy9fbGF5b3V0cy8xNS9vbmVkcml2ZS5hc3B4P2dhPTEmaWQ9JTJGcGVyc29uYWwlMkZzaW5jZTI1MDElNUZzaW5jZTE5ODIlNUZvcmclMkZEb2N1bWVudHMlMkZQMTYyNCUyMOi3n&amp;#43;eGiua1qeWtpuayn&amp;#43;mAmsK3MzDorrIiPui3n&amp;#43;eGiua1qeWtpuayn&amp;#43;mAmsK3MzDorrI8L2E&amp;#43;CjwvbGk&amp;#43;CjxsaT4KPGEgaHJlZj0iaHR0cHM6Ly9ib290c29vbi5naXRodWIuaW8iPuiWm&amp;#43;WFhuS4sOe7j&amp;#43;a1juWtpjwvYT4KPC9saT4KPGxpPiA8YSBocmVmPSJodHRwczovL2Jvb2tzLmNpbmFzLmNuOjgwMDAiPuaAquivnuihjOS4uuWtpjwvYT48L2xpPgoKPC91bD4K"
 >
 已加密，请输入密码
 &lt;/div>
&lt;/div>

&lt;script>
 
 async function decrypt() {
 try {
 const input = document.getElementById('crypto-password');
 const output = document.getElementById('crypto-output');
 const form = document.querySelector('.crypto-form');

 
 if(!input?.value) {
 showError('请输入密码');
 return;
 }

 const password = input.value;
 const publishDate = "303500303";
 const hash = await sha256(password + publishDate);
 const targetHash = await sha256( "caoyang" + publishDate);

 if(hash !== targetHash) {
 throw new Error('密码错误');
 }

 const base64 = output.getAttribute('data-content');
 const decoder = new TextDecoder();
 const decoded = decoder.decode(Uint8Array.from(atob(base64), c => c.charCodeAt(0)));
 output.innerHTML = decoded;
 console.log(decoded);
 form.style.display = 'none';
 } catch(e) {
 console.log(e)
 showError('密码错误，请重试');
 }
 }

 
 async function sha256(text) {
 const encoder = new TextEncoder();
 const data = encoder.encode(text);
 const hash = await crypto.subtle.digest('SHA-256', data);
 return Array.from(new Uint8Array(hash))
 .map(b => b.toString(16).padStart(2, '0'))
 .join('');
 }

 
 function showError(message) {
 const output = document.getElementById('crypto-output');
 output.textContent = message;
 output.className = 'encrypted-content error-msg';
 }
&lt;/script>

